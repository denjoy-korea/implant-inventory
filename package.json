{
  "name": "임플란트-재고관리-시스템-with-dentweb",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "typecheck": "tsc --noEmit",
    "lint": "npm run typecheck && node scripts/lint-check.mjs",
    "test": "node --test scripts/*.test.mjs",
    "test:funnel": "node --test scripts/funnel-kpi-regression.test.mjs",
    "test:legalux": "node --test scripts/legal-ux-hardening.test.mjs",
    "report:traffic": "node scripts/admin-traffic-snapshot.mjs 30",
    "smoke:ops": "node scripts/operational-smoke-checklist.mjs",
    "smoke:edge": "node scripts/check-edge-functions.mjs",
    "smoke:edge:strict": "node scripts/check-edge-functions.mjs --require-env --fail-on-unreachable",
    "verify:premerge": "npm run smoke:ops && npm run lint && npm run test && npm run build",
    "verify:release": "npm run smoke:edge:strict && npm run verify:premerge",
    "build": "node scripts/generate-release-manifest.mjs && npm run typecheck && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.95.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-helmet-async": "^2.0.5"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^22.14.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.0.0",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "xlsx": "^0.18.5"
  }
}
